<?xml version="1.0" encoding="utf-8" ?>
<root xmlns="http://rsdn.ru/projects/Janus/JanusMenuDescriptor.xsd"
	  name="JBrowser.Menu">

	<group name="Main">
		<menu name="File" textResource="File.Text">
			<group name="Close">
				<menuCommand command="Janus.JBrowser.Close" image="exit"/>
			</group>
		</menu>

		<menu name="Navigation" textResource="Navigation.Text">
			<group name="Browsing">
				<menuCommand command="Janus.JBrowser.NavigateBackward" image="prev"/>
				<menuCommand command="Janus.JBrowser.NavigateForward" image="next"/>
				<menuCommand command="Janus.JBrowser.Refresh" image="refresh"/>
				<menuCommand command="Janus.JBrowser.Stop" image="stop"/>
			</group>

			<group name="OpenPageIn">
				<menuCommand command="Janus.JBrowser.OpenInExternalBrowser" image="ExtBrowser"/>
				<menuCommand command="Janus.Forum.GoToMessage" image="openurl"/>
			</group>
		</menu>
	</group>

</root>